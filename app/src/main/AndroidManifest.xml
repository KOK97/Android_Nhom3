        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Nhom3_Project"
        tools:targetApi="31">
        <activity

            android:name=".PayActivity"
            android:exported="false" />
        <activity
